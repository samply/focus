// Return a list of all Laboratories.
define LaboratoryList:
    if InInitialPopulation then 
        [CareTeam] O 
            where StartsWith(O.name,'Laboratory')
    else 
        {} as List<CareTeam>

// Return the ID for the given laboratory CareTeam
define function LaboratoryId(careTeam FHIR.CareTeam):
    SensibleString(careTeam.id)

// Check to see if there are any CareTeam resources with the
// given ID and names that start with "Laboratory"
define function ExistsLaboratoryId(id_var String):
    exists from [CareTeam] O
        where LaboratoryId(O) = id_var
        and StartsWith(O.name,'Laboratory')
